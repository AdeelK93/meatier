{
  "name": "meatier",
  "version": "0.5.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node ./src/server/server.babel.js",
    "prod": "NODE_ENV=production node ./src/server/server.babel.js",
    "build": "concurrent \"npm run build:client\" \"npm run build:server\"",
    "bs": "concurrent \"npm run build:server\" \"npm run prod\"",
    "build:client": "rimraf build && NODE_ENV=production webpack --colors --config ./webpack/prod.babel.js",
    "build:server": " rimraf serverBuild && NODE_ENV=production webpack --colors --config ./webpack/server.babel.js",
    "resetdb": "node ./src/server/database/resetdb.babel.js",
    "pretest": "NODE_ENV=testing node ./tests/pretest.babel.js",
    "posttest": "NODE_ENV=testing node ./tests/pretest.babel.js",
    "test": "NODE_ENV=testing ava ./src/**/__tests__/**/*.js",
    "lint": "xo src/**/*.js --plugin=react --esnext --space --fix",
    "profile": "webpack --config ./webpack/prod.babel.js --profile --json > stats.json"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "ava": "^0.8.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-helper-builder-react-jsx": "^6.3.13",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.3.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "classnames": "^2.2.1",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint-plugin-react": "3.11.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "fake-style-loader": "^1.0.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.8.0",
    "postcss-modules-values": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0",
